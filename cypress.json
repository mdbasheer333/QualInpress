{
    "baseUrl": "http://tabler-react.com",
    "env": null,
    "fixturesFolder": "./testdata",
    "integrationFolder": ".",
    "testFiles": "./testspecs/react.test/**/*.spec.js*",
    "ignoreTestFiles": "./testspecs/*.skip.spec.js",
    "pluginsFile": "./libraries/plugins",
    "screenshotsFolder": "./testresults/reports/screenshots",
    "supportFile": "./libraries/support",
    "videosFolder": "./testresults/videos",
    "componentFolder": "./component",
    "pageLoadTimeout": 60000,
    "viewportWidth": 1000,
    "requestTimeout": 5000,
    "viewportHeight": 660,
    "responseTimeout": 30000,
    "animationDistanceThreshold": 5,
    "fileServerFolder": "",
    "blockHosts": null,
    "chromeWebSecurity": true,
    "modifyObstructiveCode": true,
    "hosts": null,
    "numTestsKeptInMemory": 50,
    "port": null,
    "projectId": null,
    "reporter": "mochawesome",
    "reporterOptions": {
        "reportDir": "./testresults/reports/mocha",
        "reportFilename": "testresult",
        "overwite": true,
        "html": true,
        "json": true,
        "charts": true,
        "timestamp": "mmddyyyy_HHMMss",
        "quiet": true,
        "reportTitle": "QualInpress test results",
        "mochawesomeReporterOptions": {
            "overwrite": false
        },
        "overwrite": false
    },
    "defaultCommandTimeout": 4000,
    "trashAssetsBeforeRuns": true,
    "execTimeout": 60000,
    "userAgent": null,
    "video": false,
    "taskTimeout": 60000,
    "videoCompression": 32,
    "videoUploadOnPasses": true,
    "watchForFileChanges": true,
    "waitForAnimations": true,
    "nodeVersion": "default",
    "experimentalSourceRewriting": false,
    "experimentalFetchPolyfill": false
}
